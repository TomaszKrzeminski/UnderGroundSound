@model Province
@{
    ViewData["Title"] = "CountySelectListView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-group">
    <label>Województwo</label>
    <select class="form-control " id="SelectProvince" name="MusicGenre">
        @foreach (var county in Model.CountyList)
        {
            <option id="x" class="ProvinceSelect">@county.CountyName</option>

        }
    </select>
</div>

